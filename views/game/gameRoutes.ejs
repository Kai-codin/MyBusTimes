<h1><%= game %> Routes</h1>
<div class="game-route">
  <% if (routes.length > 0) { %>
  <% routes.forEach(route => { %>
  <a href="/game/<%= game %>/route/<%= route["Route Num"] %>">
    <div class="route-card">
      <strong><%= route["Route Num"] %> </strong><br>
      <ul>
        <li><strong>Inbound:</strong> <%= route.Inbound["In Dest"] %></li>
        <li><strong>Outbound:</strong> <%= route.Inbound["Out Dest"] %></li>
      </ul>
    </div>
  </a>
  <% }); %>
  <% } else { %>
  <p>No routes found.</p>
  <% } %>
</div>